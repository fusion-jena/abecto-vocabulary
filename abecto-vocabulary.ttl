@prefix av: <http://w3id.org/abecto-vocabulary#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dqv: <http://www.w3.org/ns/dqv#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ldqd: <http://www.w3.org/2016/05/ldqd#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix p-plan: <http://purl.org/net/p-plan#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfg: <http://www.w3.org/2004/03/trix/rdfg-1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vaem: <http://www.linkedmodel.org/schema/vaem#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://w3id.org/abecto-vocabulary> a owl:Ontology ;
    vann:preferredNamespaceUri "http://w3id.org/abecto-vocabulary#";
    vann:preferredNamespacePrefix "av";
    dcterms:title "ABECTO Vocabulary"@en;
    dcterms:description "The ABECTO Vocabulary (AV) is a vocabulary for the description of execution plans and results of the ABox Evaluation and Comparison Tool for Ontologies (ABECTO)."@en ;
    # foaf:depiction
    # foaf:homepage
    dcterms:creator "Jan Martin Keil" ; # hotfix for https://github.com/dgarijo/Widoco/issues/285
    # dcterms:creator [a foaf:Person; foaf:name "Jan Martin Keil"; foaf:mbox <mailto:jan-martin.keil@uni-jena.de>; owl:sameAs <http://orcid.org/0000-0002-7733-0193>] ;
    # dcterms:contributor
    dcterms:license <http://creativecommons.org/licenses/by/4.0/> ;
    dcterms:created "2021-05-02"^^xsd:date ;
    owl:versionInfo "0.1" ;
    owl:versionIRI <http://w3id.org/abecto-vocabulary/0.1> ;
    # owl:priorVersion 
    .

av:Step a rdfs:Class ;
    rdfs:subClassOf p-plan:Step ;
    rdfs:label "step"@en ;
    rdfs:comment "A single step in an ABECTO execution plan defined by the processor class, preceding steps (p-plan:isPrecededBy), parameters (av:hasParameter), and input models (av:inputMetaDataGraph)."@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:StepExecution a rdfs:Class ;
    rdfs:subClassOf p-plan:Activity ;
    rdfs:label "step execution"@en ;
    rdfs:comment "A step execution is an activity that executes a single step of a plan. It is determined by the correspnding step (p-plan:corresondsToStep), start and end date time (prov:startedAtTime, prov:endedAtTime), and the used primary and meta data graphs (prov:used)."@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:Parameter a rdfs:Class ;
    rdfs:label "parameter"@en ;
    rdfs:comment "A generic parameter defined by a key String (av:key) and a value literal (rdf:value)."@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:Aspect a rdfs:Class ;
    rdfs:label "aspect"@en ;
    rdfs:comment "An aspect denotes groups of facts from multiple datasets belonging to corresponding features or classes of resources."@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:AspectPattern a rdfs:Class ;
    rdfs:label "aspect pattern"@en ;
    rdfs:comment "An aspect pattern determines the facts from a single dataset (av:associatedDataset) belonging to a specific aspect (av:ofAspect) using a SPARQL select query (av:definingQuery)."@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:Deviation a rdfs:Class ;
    rdfs:label "deviation"@en ;
    rdfs:comment "A deviation is the body (oa:hasBody) of a dqv:QualityAnnotation on a resource (oa:hasTarget). It denotes a mismatch of the value (av:affectedValue) of a specified variable (av:affectedVariableName) from a specified aspect (av:affectedAspect) to the corresponding value (av:comparedToValue) of a corresponding resource (av:comparedToResource) from a specified dataset (av:comparedToDataset)."@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:ValueOmission a rdfs:Class ;
    rdfs:label "value omission"@en ;
    rdfs:comment "A value omission is the body (oa:hasBody) of a dqv:QualityAnnotation on a resource (oa:hasTarget). It denotes the absent of a certain value for a specified variable (av:affectedVariableName) from a specified aspect (av:affectedAspect) in comparison to a value (av:comparedToValue) of a corresponding resource (av:comparedToResource) from a specified dataset (av:comparedToDataset)."@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:ResourceOmission a rdfs:Class ;
    rdfs:label "resource omission"@en ;
    rdfs:comment "A resource omission is the body (oa:hasBody) of a dqv:QualityAnnotation on a dataset (oa:hasTarget). It denotes the absent of a corresponding resource from a specified aspect (av:affectedAspect) in comparison to a resource (av:comparedToResource) from a specified dataset (av:comparedToDataset)."@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:Issue a rdfs:Class ;
    rdfs:label "issue"@en ;
    rdfs:comment "An issue is the body (oa:hasBody) of a dqv:QualityAnnotation on a resource or a dataset (oa:hasTarget). It describes an issue of a certain type (av:issueType) with a description (rdfs:comment). An issue might affect a certain aspect (av:Aspect) or a certain aspect variable (av:affectedVariableName)."@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:CorrespondenceSet a rdfs:Class ;
    rdfs:label "correspondence set"@en ;
    rdfs:comment "A correspondence set is a set that contains resources (av:containedResource) from a common aspect (av:affectedAspect) that represent all the same concepts. This indicates a high degree of confidence that the linked concepts can be used interchangeably across a wide range of applications (just like [skos:exactMatch](https://www.w3.org/TR/skos-reference/#mapping))."@en ;
    vaem:rationale """The reuse of owl:sameAs and owl:equivalentClass was rejected due to the following reasons:
* Different representations of equivalent concepts might not be exactly equivalent. Especially, they might be individuals in one dataset and classes in another dataset.
* They do not enable an assignment to an aspect.

The reuse of the [Alignment API format](https://moex.gitlabpages.inria.fr/alignapi/format.html) vocabulary and [EDOLA](http://ns.inria.org/edoal/1.0/) vocabulary was rejected due to the following reasons:
* It is focused on the representation of alignments of exactly two datasets represented by exactly one file source.
* They do not enable an assignment to an aspect.

The reuse of [skos:exactMatch](https://www.w3.org/TR/skos-reference/#mapping) was rejected due to the following reasons:
* It does not enable an assignment to an aspect."""@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:IncorrespondenceSet a rdfs:Class ;
    rdfs:label "incorrespondence set"@en ;
    rdfs:comment "A incorrespondence set is a set that contains resources (av:containedResource) that represent all pairwise different concepts."@en ;
    vaem:rationale """The reuse of owl:differentFrom, owl:disjointWith, and  owl:AllDisjointClasses was rejected due to the following reasons:
* Different representations of resources for one aspect might be individuals in one dataset and classes in another dataset.
* They do not enable an assignment to an aspect.

The reuse of the [Alignment API format](https://moex.gitlabpages.inria.fr/alignapi/format.html) vocabulary and [EDOLA](http://ns.inria.org/edoal/1.0/) vocabulary was rejected due to the following reasons:
* It is focused on the representation of alignments of exactly two datasets represented by exactly one file source.
* They do not enable an assignment to an aspect."""@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:MetaDataGraph a rdfs:Class ;
    rdfs:subClassOf rdfg:Graph , prov:Entity ;
    rdfs:label "meta data graph"@en ;
    rdfs:comment "A meta data graph is a RDF graph that contains meta data of a dataset (av:associatedDataset)."@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:PrimaryDataGraph a rdfs:Class ;
    rdfs:subClassOf rdfg:Graph , prov:Entity ;
    rdfs:label "primary data graph"@en ;
    rdfs:comment "A meta data graph is a RDF graph that contains primary data of a dataset (av:associatedDataset)."@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .

av:hasParameter a owl:ObjectProperty ;
    rdfs:label "has parameter"@en ;
    rdfs:comment ""@en ;
    rdfs:range av:Parameter ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:ofAspect a owl:ObjectProperty ;
    rdfs:label "of aspect"@en ;
    rdfs:comment ""@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:affectedAspect a owl:ObjectProperty ;
    rdfs:label "affected aspect"@en ;
    rdfs:comment ""@en ;
    rdfs:range av:Aspect ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:associatedDataset a owl:ObjectProperty ;
    rdfs:label "associated dataset"@en ;
    rdfs:comment ""@en ;
    rdfs:range dcat:Dataset ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:comparedToDataset a owl:ObjectProperty ;
    rdfs:label "compared to dataset"@en ;
    rdfs:comment ""@en ;
    rdfs:range dcat:Dataset ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:comparedToResource a owl:ObjectProperty ;
    rdfs:label "compared to resource"@en ;
    rdfs:comment ""@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:containedResource a owl:ObjectProperty ;
    rdfs:label "contained resource"@en ;
    rdfs:comment ""@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:inputMetaDataGraph a owl:ObjectProperty ;
    rdfs:label "input meta data graph"@en ;
    rdfs:comment ""@en ;
    rdfs:domain av:Step ;
    rdfs:range av:MetaDataGraph ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .

av:JavaClass a rdfs:Datatype ;
    rdfs:subClassOf xsd:string ;
    rdfs:label "Java class"@en ;
    rdfs:comment ""@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:SparqlSelectQuery a rdfs:Datatype ;
    rdfs:subClassOf xsd:string ;
    rdfs:label "SPARQL select query"@en ;
    rdfs:comment ""@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .

av:definingQuery a owl:DatatypeProperty ;
    rdfs:label "defining query"@en ;
    rdfs:comment ""@en ;
    rdfs:domain av:AspectPattern ;
    rdfs:range av:SparqlSelectQuery ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:key a owl:DatatypeProperty ;
    rdfs:label "key"@en ;
    rdfs:comment ""@en ;
    rdfs:domain av:Parameter ;
    rdfs:range xsd:string ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:keyVariableName a owl:DatatypeProperty ;
    rdfs:label "key variable name"@en ;
    rdfs:comment ""@en ;
    rdfs:domain av:Aspect ;
    rdfs:range xsd:string ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:affectedVariableName a owl:DatatypeProperty ;
    rdfs:label "affected variable name"@en ;
    rdfs:comment ""@en ;
    rdfs:range xsd:string ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:affectedValue a owl:DatatypeProperty ;
    rdfs:label "affected value"@en ;
    rdfs:comment ""@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:comparedToValue a owl:DatatypeProperty ;
    rdfs:label "compared to value"@en ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:issueType a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:label "issue type"@en ;
    rdfs:comment ""@en ;
    rdfs:range xsd:string ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:processorClass a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:label "processor class"@en ;
    rdfs:comment ""@en ;
    rdfs:domain av:Step ;
    rdfs:range av:JavaClass ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .

av:relativeCompleteness a dqv:Metric ;
    rdfs:label "relative completeness"@en ;
    rdfs:comment ""@en ;
    dqv:inDimension ldqd:completeness ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:relativeCoverage a dqv:Metric ;
    rdfs:label "relative coverage"@en ;
    rdfs:comment ""@en ;
    dqv:inDimension ldqd:completeness ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:absoluteCoverage a dqv:Metric ;
    rdfs:label "absolute coverage"@en ;
    rdfs:comment ""@en ;
    dqv:inDimension ldqd:completeness ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
av:count a dqv:Metric ;
    rdfs:label "count"@en ;
    rdfs:comment ""@en ;
    dqv:inDimension ldqd:completeness ;
    rdfs:isDefinedBy <http://w3id.org/abecto-vocabulary> ;
    .
